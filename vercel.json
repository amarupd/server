{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/fillingdata/(.*)",
        "dest": "/api/data/$1"
      },
      {
        "src": "/dashboard/(.*)",
        "dest": "/api/dashboard/$1"
      },
      {
        "src": "/database/(.*)",
        "dest": "/api/config/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  