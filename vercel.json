{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/data/(.*)",
        "dest": "/api/data/$1"
      },
      {
        "src": "/api/dashboard/(.*)",
        "dest": "/api/dashboard/$1"
      },
      {
        "src": "/api/config/(.*)",
        "dest": "/api/config/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  